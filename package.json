{
  "name": "@thadeu/logger-storage",
  "version": "0.0.13",
  "description": "console.log out of the box, send to localStorage easily",
  "keywords": [
    "console",
    "logger-storage",
    "logger storage",
    "export log",
    "log chrome"
  ],
  "author": "Thadeu Esteves Jr",
  "main": "dist/logger-storage.js",
  "module": "src/logger-storage.js",
  "files": [
    "src/**/*",
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "start": "webpack-dev-server --progress --mode development --watch-content-base --config webpack.config.dev.js",
    "build:minified": "env NODE_ENV=production ./node_modules/.bin/webpack --mode production",
    "build:expanded": "env NODE_ENV=production ./node_modules/.bin/webpack",
    "build": "npm test && npm build:minified && npm build:expanded",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./config/setupTests.js",
      "./config/browserMocks.js"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/__spec__/**/*.{js,jsx,mjs}",
      "<rootDir>/test/**/*.{js,jsx,mjs}",
      "<rootDir>/spec/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thadeu/logger-storage.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jsdom": "^11.10.0",
    "snap-shot": "^2.17.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "bugs": {
    "url": "https://github.com/thadeu/logger-storage/issues"
  },
  "homepage": "https://github.com/thadeu/logger-storage#readme",
  "dependencies": {}
}
